
<div class="header">
  <div class="container">
    <h1>Edit message</h1>
  </div>
</div>

<div class="edit">
  <div class="container">
    <%=form_for @message, :url => "/messages/update/"+(@message.id.to_s) do |f| %>
        <%= f.label :Message%>
        <%= f.text_field :content%>
        <%= f.submit "Edit Message"%>
    <%end %>
  </div>
  <%= link_to 'See Index',"/messages"%>
</div>
